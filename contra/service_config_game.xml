<seelog type="sync" minlevel="trace" maxlevel="error">  
   <outputs formatid="common">  

   <buffered formatid="common" size="10000" flushperiod="1000">  

	   <rollingfile type="date" filename="/data/log/threeCountries/service/game_test.log" datepattern="02.01.2006" fullname="true" maxrolls="30"/>

   </buffered>  

   </outputs>  

    <formats>  
        <format id="common" format="%Time [%l] [%File:%Line] [%Func] %Msg%n" />  
    </formats>  
</seelog>  
